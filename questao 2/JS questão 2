function exibirNum(){
  let num = document.getElementById("number").value;
  let imprime = document.getElementById("numOutput");
  const numList = parseInt(number);
 
  if (num == ""){
    imprime.innerHTML = "Por favor, insira um número."
  } else {
    for (let i  = 0; i <= num; i++){
      const itemLista = document.createElement('li');
     
      itemLista.textContent = "Letícia - ${i}";
     
  }
  
